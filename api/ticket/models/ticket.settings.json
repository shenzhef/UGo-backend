{
  "kind": "collectionType",
  "collectionName": "tickets",
  "info": {
    "name": "ticket"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "date": {
      "type": "date"
    },
    "user": {
      "plugin": "users-permissions",
      "model": "user"
    },
    "message": {
      "type": "string"
    },
    "category": {
      "type": "string"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "resuelto",
        "pendiente"
      ]
    }
  }
}
