{
  "kind": "collectionType",
  "collectionName": "feeds",
  "info": {
    "name": "feed",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "user": {
      "plugin": "users-permissions",
      "model": "user"
    },
    "paseador": {
      "plugin": "users-permissions",
      "model": "user"
    },
    "date": {
      "type": "datetime"
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "solicitud",
        "mensaje",
        "alerta"
      ]
    },
    "start": {
      "type": "time"
    },
    "end": {
      "type": "time"
    },
    "direccion": {
      "type": "json"
    },
    "transaction": {
      "via": "feeds",
      "model": "transaction"
    },
    "bundleID": {
      "type": "string",
      "unique": false
    },
    "dog": {
      "model": "dog"
    },
    "isAccepted": {
      "type": "boolean"
    },
    "cancelled": {
      "type": "component",
      "repeatable": false,
      "component": "cancelation.cancelled"
    },
    "comments": {
      "type": "string"
    },
    "payment_info": {
      "type": "component",
      "repeatable": false,
      "component": "payments.payment-status"
    }
  }
}
